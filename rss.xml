select stat;&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;Where&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;c#&quot;&gt;&lt;pre class=&quot;language-c#&quot;&gt;&lt;code class=&quot;language-c#&quot;&gt;#lambda
items.Where(item =&amp;gt; item.count &amp;lt; 10)

#query
from item in items
where item.count &amp;lt; 10
select item&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;Orderby&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;c#&quot;&gt;&lt;pre class=&quot;language-c#&quot;&gt;&lt;code class=&quot;language-c#&quot;&gt;#lambda
items.OrderBy(item =&amp;gt; item.Type).ThenByDescending(item =&amp;gt; item.Stats.Average())

#query
from item in items
orderby item.Type ascending, item.Stats.Average() descending
select item&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;GroupBy&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;c#&quot;&gt;&lt;pre class=&quot;language-c#&quot;&gt;&lt;code class=&quot;language-c#&quot;&gt;#lambda
items.GroupBy(item =&amp;gt; item.Type)

#query
from item in items
group item by item.Type into g
select g&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;Take&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;c#&quot;&gt;&lt;pre class=&quot;language-c#&quot;&gt;&lt;code class=&quot;language-c#&quot;&gt;#lambda
items.Take(2)

#query
(from item in items
 select item).Take(2)&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;Skip&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;c#&quot;&gt;&lt;pre class=&quot;language-c#&quot;&gt;&lt;code class=&quot;language-c#&quot;&gt;#lambda
items.Skip(2)

#query
(from item in items
 select item).Skip(2)&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;</content:encoded></item><item><title><![CDATA[GitHub 블로그 만들기 by Gatsby]]></title><description><![CDATA[조건 github repository(username/github.io) npm node.js gatsby gh-pages 설치 Gatsby 코드 수정 GitHub]]></description><link>https://swedendry.github.io/github-blog-by-gatsby/</link><guid isPermaLink="false">https://swedendry.github.io/github-blog-by-gatsby/</guid><pubDate>Thu, 14 Nov 2019 17:00:00 GMT</pubDate><content:encoded>&lt;h2&gt;조건&lt;/h2&gt;
&lt;ul&gt;
&lt;li&gt;github repository(username/github.io)&lt;/li&gt;
&lt;li&gt;npm&lt;/li&gt;
&lt;li&gt;node.js&lt;/li&gt;
&lt;li&gt;gatsby&lt;/li&gt;
&lt;li&gt;gh-pages&lt;/li&gt;
&lt;/ul&gt;
&lt;h2&gt;설치&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;text&quot;&gt;&lt;pre class=&quot;language-text&quot;&gt;&lt;code class=&quot;language-text&quot;&gt;#gatsby 설치
npm install -g gatsby-cli

#gh-pages 설치
npm install gh-pages --save-dev&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;Gatsby&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;text&quot;&gt;&lt;pre class=&quot;language-text&quot;&gt;&lt;code class=&quot;language-text&quot;&gt;#https://github.com/gatsbyjs/gatsby-starter-blog 샘플 설치
gatsby new my-blog-starter https://github.com/gatsbyjs/gatsby-starter-blog
gatsby develop

#localhost:8000/ 확인&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;코드 수정&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;text&quot;&gt;&lt;pre class=&quot;language-text&quot;&gt;&lt;code class=&quot;language-text&quot;&gt;#package.json deploy 추가
 &amp;quot;scripts&amp;quot;: {
     ~~~
    &amp;quot;deploy&amp;quot;: &amp;quot;gatsby build --prefix-paths &amp;amp;&amp;amp; gh-pages -d public -b master&amp;quot;
  }

#gatsby-config.js 수정
#package.json 수정&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;
&lt;h2&gt;GitHub&lt;/h2&gt;
&lt;div class=&quot;gatsby-highlight&quot; data-language=&quot;text&quot;&gt;&lt;pre class=&quot;language-text&quot;&gt;&lt;code class=&quot;language-text&quot;&gt;#develop branch에 개발 리소스 올리기
git remote add origin `repository`
git branch develop
git checkout develop
git push -u origin develop

#master branch에 빌드 리소스 올리기
npm run deploy

#username/github.io/ 확인&lt;/code&gt;&lt;/pre&gt;&lt;/div&gt;</content:encoded></item></channel></rss>